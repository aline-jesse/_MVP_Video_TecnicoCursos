    "test:playwright:ui": "playwright test --ui",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:debug": "playwright test --debug",
    "test:playwright:chromium": "playwright test --project=chromium",
    "test:playwright:firefox": "playwright test --project=firefox",
    "test:playwright:webkit": "playwright test --project=webkit",
    "test:playwright:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:playwright:report": "playwright show-report qa/artifacts/html-report",
    "docker:setup": "powershell -ExecutionPolicy Bypass -File ../scripts/docker-setup.ps1",
    "docker:setup:bash": "bash ../scripts/docker-setup.sh",
    "docker:up": "cd .. && docker-compose up -d",
    "docker:down": "cd .. && docker-compose down",
    "docker:logs": "cd .. && docker-compose logs -f",
    "docker:restart": "cd .. && docker-compose restart",
    "docker:clean": "cd .. && docker-compose down -v --remove-orphans",
    "audio2face:status": "curl -f http://localhost:8012/health || echo 'Audio2Face not available'",
    "redis:status": "redis-cli -h localhost -p 6379 ping || echo 'Redis not available'",
    "infrastructure:check": "npm run audio2face:status && npm run redis:status",
    "websocket:start": "tsx scripts/start-websocket.ts",
    "websocket:dev": "tsx --watch scripts/start-websocket.ts"
