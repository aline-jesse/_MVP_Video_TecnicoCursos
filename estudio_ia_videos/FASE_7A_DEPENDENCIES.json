{
  "name": "estudio-ia-videos-refinement",
  "version": "1.0.0",
  "description": "Instalação de dependências da Fase 7A - Refinamento Total",
  "scripts": {
    "install-refinement": "npm install canvas@^2.11.2 axios@^1.6.2 @aws-sdk/client-s3@^3.462.0",
    "install-types": "npm install -D @types/canvas@^1.6.0",
    "install-all": "npm run install-refinement && npm run install-types",
    "test": "echo 'Fase 7A instalada com sucesso!'"
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "axios": "^1.6.2",
    "@aws-sdk/client-s3": "^3.462.0"
  },
  "devDependencies": {
    "@types/canvas": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ]
}
