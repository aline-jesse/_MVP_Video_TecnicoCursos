{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "estudio_ia_videos/app/*"
      ]
    },
  "ignoreDeprecations": "6.0",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
  "estudio_ia_videos/app/app/**/*.ts",
  "estudio_ia_videos/app/app/**/*.tsx",
  "estudio_ia_videos/app/lib/**/*.ts",
  "estudio_ia_videos/app/lib/**/*.tsx",
  "estudio_ia_videos/app/types/**/*.ts",
  "estudio_ia_videos/app/types/**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "**/__tests__/**",
    "estudio_ia_videos/src",
    "estudio_ia_videos/tests",
    "estudio_ia_videos/app/archive/legacy",
    "app",
    "app/**/*"
  ]
}
